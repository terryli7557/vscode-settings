{
  "window.zoomLevel": 2,
  "typescript.tsserver.log": "verbose",

  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.detectIndentation": true,
  "editor.parameterHints.enabled": false,
  "terminal.integrated.fontSize": 9,
  "editor.tabSize": 2,
  "files.exclude": {
    "USE_GITIGNORE": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "*",
      "local": "en-US",
      "dictionaries": ["wordsEn"]
    }
  ],
  "workbench.activityBar.visible": false,
  "local-history.absolute": true,
  "local-history.path": "/Users/terryli/.vscode-history",
  "workbench.statusBar.visible": false,
  "editor.renderLineHighlight": "all",
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#080200",
    "scrollbarSlider.hoverBackground": "#222",
    "editor.background": "#080200",
    "editor.lineHighlightBackground": "#080200",
    "editorGroup.border": "#080200"
  },
  "editor.overviewRulerBorder": false,
  "workbench.editor.showTabs": false,
  "git.enabled": false,
  "editor.lineNumbers": "off",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "settings.cycle": [
    {
      "id": "lineNumbers",
      "values": [
        {
          "editor.lineNumbers": "off"
        },
        {
          "editor.lineNumbers": "on"
        }
      ]
    }
  ],
  "editor.glyphMargin": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "cSpell.userWords": ["noopener", "noreferrer", "statechange"],
  "screencastMode.onlyKeyboardShortcuts": true,
  "emmet.showExpandedAbbreviation": "never",
  "editor.snippetSuggestions": "bottom",
  "files.associations": {
    "*.code-snippets": "json",
    "*.ts": "typescript"
  },
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "onlySnippets",
  "terminal.integrated.shellArgs.osx": [],
  "files.autoSave": "onFocusChange",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showWords": false,
  "editor.suggest.showClasses": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showIcons": false,
  "editor.suggest.showIssues": false,
  "editor.suggest.showKeywords": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showMethods": false,
  "editor.suggest.showValues": false,
  "javascript.suggest.autoImports": false,
  // "editor.suggest.showEnums": false,
  // "editor.suggest.showFields": false,
  // "editor.suggest.showFolders": false,
  // "editor.suggest.showInterfaces": false,
  // "editor.suggest.showModules": false,
  // "editor.suggest.showConstants": false,
  // "editor.suggest.showFiles": false,

  "editor.suggest.showFiles": true,
  "editor.suggest.showEnums": true,
  "editor.suggest.showFields": true,
  "editor.suggest.showFolders": true,
  "editor.suggest.showInterfaces": true,
  "editor.suggest.showModules": true,
  "editor.suggest.showConstants": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "workbench.colorTheme": "Oceanic Next",
  "editor.folding": false
}
