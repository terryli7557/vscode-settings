{
  "window.zoomLevel": 1,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.detectIndentation": true,
  "editor.parameterHints.enabled": false,
  "editor.snippetSuggestions": "top",
  "editor.wordBasedSuggestions": false,
  "terminal.integrated.fontSize": 13,

  "editor.tabSize": 2,
  "editor.folding": false,
  "files.exclude": {
    "USE_GITIGNORE": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },

  "editor.formatOnSave": false,
  "[javascript]": {
    "editor.suggestSelection": "recentlyUsed",
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },

  "eslint.autoFixOnSave": true,

  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "*",
      "local": "en-US",
      "dictionaries": ["wordsEn"]
    }
  ],
  "workbench.activityBar.visible": false,
  "local-history.absolute": true,
  "local-history.path": "/Users/terryli/.vscode-history",
  "workbench.statusBar.visible": false,
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#080200",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.background": "#080200",
    "editor.lineHighlightBackground": "#181818",
    "editorGroup.border": "#080200",
    "scrollbarSlider.hoverBackground": "#080200"
  },
  "editor.overviewRulerBorder": false,
  "workbench.colorTheme": "Oceanic Next",
  "workbench.editor.showTabs": false,
  "git.enabled": false,
  "editor.lineNumbers": "off",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
