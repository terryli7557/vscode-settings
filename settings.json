{
  "window.zoomLevel": 2,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.detectIndentation": true,
  "editor.parameterHints.enabled": false,
  "editor.wordBasedSuggestions": false,
  "terminal.integrated.fontSize": 13,
  "editor.tabSize": 2,
  "editor.folding": false,
  "files.exclude": {
    "USE_GITIGNORE": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "*",
      "local": "en-US",
      "dictionaries": ["wordsEn"]
    }
  ],
  "workbench.activityBar.visible": false,
  "local-history.absolute": true,
  "local-history.path": "/Users/terryli/.vscode-history",
  "workbench.statusBar.visible": false,
  "editor.renderLineHighlight": "all",
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#080200",
    "scrollbarSlider.hoverBackground": "#222",
    "editor.background": "#080200",
    "editor.lineHighlightBackground": "#080200",
    "editorGroup.border": "#080200"
  },
  "editor.overviewRulerBorder": false,
  "workbench.colorTheme": "Oceanic Next",
  "workbench.editor.showTabs": false,
  "git.enabled": false,
  "editor.lineNumbers": "on",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "settings.cycle": [
    {
      "id": "lineNumbers",
      "values": [
        {
          "editor.lineNumbers": "off"
        },
        {
          "editor.lineNumbers": "on"
        }
      ]
    }
  ],
  "editor.glyphMargin": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.emptySelectionClipboard": false,
  "zenMode.hideStatusBar": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "cSpell.userWords": ["noopener", "noreferrer", "statechange"],
  "screencastMode.onlyKeyboardShortcuts": true,
  "emmet.showExpandedAbbreviation": "never",
  "editor.snippetSuggestions": "bottom",
  "files.associations": {
    "*.code-snippets": "json"
  },
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.tabCompletion": "onlySnippets",
  "editor.quickSuggestionsDelay": 500
}

