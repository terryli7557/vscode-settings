{
  "window.zoomLevel": 2,

  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.detectIndentation": true,
  "editor.parameterHints.enabled": false,
  "terminal.integrated.fontSize": 9,
  "editor.tabSize": 2,
  "files.exclude": {
    "USE_GITIGNORE": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "*",
      "local": "en-US",
      "dictionaries": ["wordsEn"]
    }
  ],
  "workbench.activityBar.visible": false,
  "local-history.absolute": true,
  "local-history.path": "/Users/terryli/.vscode-history",
  "workbench.statusBar.visible": false,
  "editor.renderLineHighlight": "all",
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#080200",
    "scrollbarSlider.hoverBackground": "#222",
    "editor.background": "#080200",
    "editor.lineHighlightBackground": "#080200",
    "editorGroup.border": "#080200"
  },
  "editor.overviewRulerBorder": false,
  "workbench.editor.showTabs": false,
  "git.enabled": false,
  "editor.lineNumbers": "off",

  "breadcrumbs.enabled": true,
  "settings.cycle": [
    {
      "id": "lineNumbers",
      "values": [
        {
          "editor.lineNumbers": "off"
        },
        {
          "editor.lineNumbers": "on"
        }
      ]
    }
  ],
  "editor.glyphMargin": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "cSpell.userWords": [
    "abbrv",
    "abcdefjhijklmnopqrstuvwxyz",
    "actioned",
    "actioned at",
    "antd",
    "anticon",
    "at",
    "authed",
    "clonedeep",
    "csis",
    "datepicker",
    "dpicker",
    "falthy",
    "fontawesome",
    "fooby",
    "fortawesome",
    "geekblue",
    "hoportal",
    "isequal",
    "lastname",
    "libphonenumber",
    "noopener",
    "noreferrer",
    "overscan",
    "popconfirm",
    "reaptcha",
    "recaptcha",
    "shortname",
    "signup",
    "sitekey",
    "statechange",
    "stroopwafel",
    "submenu",
    "svgs",
    "testid",
    "upolader",
    "weeknames",
    "yyyymmdd"
  ],
  "screencastMode.onlyKeyboardShortcuts": true,
  "emmet.showExpandedAbbreviation": "never",
  "terminal.integrated.shellArgs.osx": [],
  "files.associations": {
    "*.code-snippets": "json",
    "*.ts": "typescript"
  },
  "files.autoSave": "onFocusChange",
  "editor.snippetSuggestions": "bottom",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "onlySnippets",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showReferences": false,
  "editor.suggest.showStructs": false,
  "editor.suggest.showUsers": false,
  "editor.suggest.showWords": false,
  "editor.suggest.showClasses": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showIssues": false,
  "editor.suggest.showKeywords": false,
  "editor.suggest.showOperators": false,
  "editor.suggest.showUnits": false,
  "editor.suggest.showValues": false,
  "javascript.suggest.autoImports": false,

  // "editor.suggest.showEnums": false,
  // "editor.suggest.showFields": false,
  // "editor.suggest.showFolders": false,
  // "editor.suggest.showInterfaces": false,
  // "editor.suggest.showModules": false,
  // "editor.suggest.showConstants": false,
  // "editor.suggest.showFiles": false,

  "editor.suggest.showMethods": true,
  "editor.suggest.showIcons": true,
  "editor.suggest.showFiles": true,
  "editor.suggest.showEnums": true,
  "editor.suggest.showFields": true,
  "editor.suggest.showFolders": true,
  "editor.suggest.showInterfaces": true,
  "editor.suggest.showModules": true,
  "editor.suggest.showConstants": true,

  "workbench.colorTheme": "Oceanic Next",
  "editor.folding": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.formatOnSave": true,
  "typescript.suggest.autoImports": false
}
